int fibonacci(int n){
    int a; int b; int c; int i;
    if (n <= 0){ return 0;}
    else{ if (n == 1){ return 1;}
    else {
        a = 0;
        b = 1;
        i = 1;
        while (i < n){
            c = b;
            b = b + a;
            a = c;
            i = i + 1;
        }
        return b;
     }
    }
}


void main(void)
{	int x;
	x = scanf();
	printf(fibonacci(x));
}



